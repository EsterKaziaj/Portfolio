{
    "compilerOptions": {
      "target": "ESNext",
      "lib": ["DOM", "DOM.Iterable", "ESNext"],
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "Node",
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "noEmit": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "resolveJsonModule": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  